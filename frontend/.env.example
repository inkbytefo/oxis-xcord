# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001/api/v1
NEXT_PUBLIC_WS_URL=ws://localhost:3002

# Authentication
NEXT_PUBLIC_JWT_EXPIRY=3600
NEXT_PUBLIC_REFRESH_TOKEN_EXPIRY=604800

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE=true
NEXT_PUBLIC_ENABLE_VIDEO=true

# External Services
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_ANALYTICS_ID=

# Performance
NEXT_PUBLIC_MAX_RECONNECT_ATTEMPTS=5
NEXT_PUBLIC_RECONNECT_INTERVAL=2000

# Cache Configuration
NEXT_PUBLIC_CACHE_MAX_AGE=3600
NEXT_PUBLIC_CACHE_STALE_WHILE_REVALIDATE=300

# Rate Limiting
NEXT_PUBLIC_RATE_LIMIT_MAX_REQUESTS=100
NEXT_PUBLIC_RATE_LIMIT_WINDOW_MS=60000